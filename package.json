{
  "name": "react-datez",
  "version": "1.3.4",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run lint && npm run clean && webpack --progress -p --config webpack.production.config.js",
    "start": "webpack-dev-server --progress --history-api-fallback --hot --inline --port 9001 --open",
    "deploy": "npm run build && npm publish",
    "dev": "npm run start",
    "dev:remote": "webpack-dev-server --progress --history-api-fallback --hot --inline --host 0.0.0.0 --port 9000 --open",
    "clean": "rimraf ./dist && mkdir dist",
    "lint": "eslint ./app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/secretyouth/react-datez.git"
  },
  "author": "Jamie Hayman <jamie@rkda.com.au>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/secretyouth/react-datez/issues"
  },
  "dependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "classnames": "^2.2.5",
    "express": "4.14.1",
    "babel-polyfill": "^6.23.0",
    "moment": "^2.18.1",
    "normalize.css": "^5.0.0",
    "react": "15.4.2",
    "react-addons-shallow-compare": "^15.5.2",
    "react-dates": "^12.1.1",
    "react-dom": "15.4.2",
    "react-redux": "^5.0.4",
    "react-router": "3.0.2",
    "redux": "^3.6.0",
    "redux-form": "^6.6.3",
    "redux-persist": "^4.8.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "6.22.1",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.22.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.9.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "0.10.0",
    "html-webpack-plugin": "2.28.0",
    "postcss": "5.2.12",
    "postcss-cssnext": "2.9.0",
    "postcss-import": "9.1.0",
    "postcss-loader": "1.3.3",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.3.0"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "plugins": [
      "react",
      "jsx-a11y",
      "import"
    ],
    "rules": {
      "comma-dangle": 0,
      "indent": [
        0,
        4
      ],
      "react/jsx-indent": [
        2,
        4
      ],
      "max-len": [
        1,
        300,
        4,
        {
          "ignorePattern": "\\s*<"
        }
      ],
      "import/no-extraneous-dependencies": 0,
      "import/prefer-default-export": 0,
      "react/jsx-filename-extension": 0,
      "react/require-extension": 0,
      "react/no-array-index-key": 0,
      "react/require-default-props": 0,
      "react/no-unescaped-entities": 0,
      "react/jsx-indent-props": 0,
      "no-console": 0,
      "semi": [
        "error",
        "never"
      ],
      "react/no-string-refs": 1,
      "no-unused-vars": [
        2,
        {
          "varsIgnorePattern": "[iI]gnored"
        }
      ],
      "no-underscore-dangle": 0,
      "no-param-reassign": 0,
      "react/forbid-prop-types": 0,
      "class-methods-use-this": 0
    },
    "env": {
      "browser": true,
      "node": true
    }
  }
}
